
function main () {
    const data = [
        "abc",
        "bcc",
        "c#c",
        "boc",
        "a#c",
        "ccc",
    ];

    let index = 0;
    let counter = 0;

    while index < data.length {
        const item = data[index];
        if item[1] == '#' {
            counter += 1;
        }
        index += 1;
    }

    print(counter);
}