
class Base {

    static new(): Base {
        return Base {};
    }

    virtual printName() {
        print("name")
    }

}

class Child extends Base {

    name: string

    static new(): Child {
        return Child {
            ...Base.new(),
            name: "Hello World",
        };
    }

    override printName() {
        print("name: ", this.name)
    }

}

function main() {
    const child = Child.new();
    child.printName();
}
